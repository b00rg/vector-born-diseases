# triatoma bug dataminer
class triatomaSpider(scrapy.Spider):
    name = 'triatoma'
    start_urls = ['https://duckduckgo.com/?q=triatome&iar=images&iax=images&ia=images',
        'https://duckduckgo.com/?q=triatome+bug&iar=images&iax=images&ia=images',
