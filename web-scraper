#img name funct 
def spam(img,eggs):
   img = img[:5]
   kool += 1 
   if (img.split(".")[1] == "jpg"):
       ext = ".jpg"
   if (img.split(".")[1] == "png"):
       ext = ".png"
   if (img.split(".")[1] == "jpeg"):
       ext = ".jpeg"
   if (img.split(".")[1] == "bmp"):
       ext = ".bmp"
   img = str(img + eggs + ext)
   
# data miner function

def miner(fDir):
    def parse(self, response):
        links = response.xpath("//img/@src")
        
        #creating paths to directories 
        x = r"\Users\rowan\OneDrive\Documents\VectorData"
        final_path = x + fDir 
        final_path = ''

        for link in links:
            url = link.get()

            if any(extension in url for extension in 
            [".jpg",".gif",".bmp",".png",".jpeg"]):
                html += """<a href="{url}"
                target=" blank">
                <img src="{url}" height="33%"
                width="33%"/>
                <a/>""".format(url=url)
                #save img to file (file = file name(defined))
                ITEM_PIPELINES = {'scrapy.pipelines.images.ImagesPipeline': 1}
                IMAGES_STORE = final_path + str(eggs+".html")
                
            for next_page in response.css('a.next'):
                if next_page is not None:
                    yield response.follow(next_page, self.parse)

